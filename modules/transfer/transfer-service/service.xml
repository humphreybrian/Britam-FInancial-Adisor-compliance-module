	<entity data-source="sampleDataSource" local-service="true" name="Foo" remote-service="false" 
	session-factory="sampleSessionFactory" table="foo" tx-manager="sampleTransactionManager 
	uuid="true""> <entity local-service="true" name="Foo" remote-service="true" 
